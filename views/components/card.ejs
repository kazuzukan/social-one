<div class="container-card">
 <h2> <%= name.value %> </h2>
    <div class="detail-title">
      <p> Nickname </p>
      <span> <%= nickname.value %> </span>
    </div>
    <br>

    <div class="detail-title">
      <p> Instagram : </p>
      <a href="https://instagram.com/<%= instagram.value %>"> @<%= instagram.value %></a>
    </div>
    
    <div class="detail-title">
      <p> Twitter : </p>
      <a href="https://twitter.com/<%= twitter.value %>"> @<%= twitter.value %> </a>
    </div>
    
    <div class="detail-title">
      <p> Facebook : </p>
      <a href="https://facebook.com/<%= facebook.value %>"> <%= facebook.value %> </a>
    </div>

    <div class="detail-title">
      <p> LinkedIn : </p>
      <span> <%= linkedIn.value %> </span>
    </div> 
    <br>
 </div>